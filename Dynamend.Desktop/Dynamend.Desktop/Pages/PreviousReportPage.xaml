<Page x:Class="Dynamend.Desktop.Pages.PreviousReportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dynamend.Desktop.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PreviousReportPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Border Grid.Column="0" Margin="10">
            <StackPanel>
                <WrapPanel>
                    <TextBlock Text="Name : "></TextBlock>
                    <TextBlock></TextBlock>
                </WrapPanel>
                <WrapPanel>
                    <TextBlock Text="Phone : "></TextBlock>
                    <TextBlock></TextBlock>
                </WrapPanel>
                <WrapPanel>
                    <TextBlock Text="Address : " ></TextBlock>
                    <TextBlock></TextBlock>
                </WrapPanel>

            </StackPanel>
        </Border>
        
    </Grid>
</Page>
