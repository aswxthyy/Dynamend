<Application x:Class="Dynamend.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dynamend.Desktop"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="TextBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="RadioButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Fonts.xaml" />
                <ResourceDictionary Source="ListStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
